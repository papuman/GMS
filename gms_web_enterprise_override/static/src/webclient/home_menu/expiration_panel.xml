<?xml version="1.0" encoding="UTF-8" ?>
<templates xml:space="preserve">

<!--
    SHAAR License Override: Remove the Odoo Enterprise expiration panel content.
    Uses template inheritance to replace the panel div, removing all Odoo.com links.
    SHAAR licensing validates locally via enterprise_subscription_service.js.
-->
<t t-inherit="DatabaseExpirationPanel" t-inherit-mode="extension">
    <xpath expr="//div[hasclass('database_expiration_panel')]" position="replace"/>
</t>

</templates>
