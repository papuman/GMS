<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">

        <!-- Electronic Invoice Sequence -->
        <record id="seq_einvoice_fe" model="ir.sequence">
            <field name="name">Factura Electrónica</field>
            <field name="code">l10n_cr.einvoice.fe</field>
            <field name="prefix">FE-</field>
            <field name="padding">10</field>
            <field name="number_increment">1</field>
            <field name="implementation">standard</field>
        </record>

        <!-- Electronic Ticket Sequence -->
        <record id="seq_einvoice_te" model="ir.sequence">
            <field name="name">Tiquete Electrónico</field>
            <field name="code">l10n_cr.einvoice.te</field>
            <field name="prefix">TE-</field>
            <field name="padding">10</field>
            <field name="number_increment">1</field>
            <field name="implementation">standard</field>
        </record>

        <!-- Credit Note Sequence -->
        <record id="seq_einvoice_nc" model="ir.sequence">
            <field name="name">Nota de Crédito Electrónica</field>
            <field name="code">l10n_cr.einvoice.nc</field>
            <field name="prefix">NC-</field>
            <field name="padding">10</field>
            <field name="number_increment">1</field>
            <field name="implementation">standard</field>
        </record>

        <!-- Debit Note Sequence -->
        <record id="seq_einvoice_nd" model="ir.sequence">
            <field name="name">Nota de Débito Electrónica</field>
            <field name="code">l10n_cr.einvoice.nd</field>
            <field name="prefix">ND-</field>
            <field name="padding">10</field>
            <field name="number_increment">1</field>
            <field name="implementation">standard</field>
        </record>


</odoo>
